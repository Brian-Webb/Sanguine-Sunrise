{
    "name": "pwnraid/bnet",
    "description": "Library for Battle.net Web APIs",
    "keywords": ["Battle.net", "Blizzard", "Warcraft", "Starcraft", "Diablo"],
    "homepage": "https://github.com/pwnraid/bnet",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Stendahl",
            "email": "jyggen@pwnraid.org"
        },
        {
            "name": "pwnRaid Community",
            "homepage": "https://github.com/pwnraid/bnet/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle": "~5.0",
        "league/oauth2-client": "0.3.*",
        "tedivm/stash": "0.12.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Pwnraid\\Bnet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pwnraid\\Bnet\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
